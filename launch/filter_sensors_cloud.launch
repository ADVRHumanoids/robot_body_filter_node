<launch>

  <node pkg="robot_body_filter_node" type="robot_body_filter_node" name="vel_body_filter_node" args="vel_">
    <rosparam command="load" file="$(find robot_body_filter_node)/config/configVelodyne.yaml" />
    <remap from="cloud_in" to="/velodyne_points" />
    <remap from="cloud_filtered" to="/velodyne_points_filtered" />  
  </node>
  
  <node pkg="robot_body_filter_node" type="robot_body_filter_node" name="d435i_body_filter_node" args="d435i_">
    <rosparam command="load" file="$(find robot_body_filter_node)/config/configD435i.yaml" />
    <remap from="cloud_in" to="/D435i_camera/depth/color/points" />
    <remap from="cloud_filtered" to="/D435i/D435i_camera/depth/color/points_filtered" />    
  </node>

</launch>
